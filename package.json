{
  "name": "graphql-intuitive-request",
  "version": "0.0.3-alpha.1",
  "description": "Intuitive and (more importantly) TS-friendly GraphQL client for queries, mutations and subscriptions",
  "homepage": "https://github.com/Snowfly-T/graphql-intuitive-request",
  "repository": {
    "type": "git",
    "url": "https://github.com/Snowfly-T/graphql-intuitive-request.git"
  },
  "license": "MIT",
  "author": "Snowflyt <gaoge011022@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "require": "./cjs/index.js",
        "types": "./types/index.d.ts"
      },
      "import": {
        "import": "./esm/index.js",
        "types": "./types/index.d.ts"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "build": "npm run clean && cpy ./src/types/**/*.d.ts ./build/types/ && cpy ./package.json ./build/ && cpy ./README.md ./build/ && cpy ./LICENSE ./build/ && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && prettier --write ./build/**/*.js && replace-in-file /\\.\\/types\\//g ./ ./build/**/*.d.ts --isRegex && eslint --fix ./build/esm/**/*.js && eslint --fix ./build/**/*.d.ts",
    "clean": "rimraf build",
    "lint": "eslint . --ext .js,.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "arktype": "^1.0.16-alpha",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "graphql-ws": "^5.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "jest": "^29.6.1",
    "prettier": "^2.8.8",
    "prettier-plugin-packagejson": "^2.4.5",
    "replace-in-file": "^7.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "ws": "^8.13.0"
  }
}
