{
  "name": "graphql-intuitive-request",
  "version": "0.0.1-rc.2",
  "author": "Snowflyt <gaoge011022@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Snowfly-T/graphql-intuitive-request.git"
  },
  "homepage": "https://github.com/Snowfly-T/graphql-intuitive-request",
  "description": "Intuitive and (more importantly) TS-friendly GraphQL client for queries, mutations and subscriptions",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && cpy ./src/types/**/*.d.ts ./build/types/ && cpy ./package.json ./build/ && cpy ./README.md ./build/ && cpy ./LICENSE ./build/ && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && prettier --write ./build/**/*.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint . --ext .js,.ts"
  },
  "type": "module",
  "types": "./index.d.ts",
  "typesVersions": {
    ">=5": {
      "*": [
        "./types/ts5/*"
      ]
    },
    ">=4": {
      "*": [
        "./types/ts4/*"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "ws": "^8.13.0"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "graphql-ws": "^5.12.0"
  }
}
