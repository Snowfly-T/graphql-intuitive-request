  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Snowfly-T/graphql-intuitive-request.git"
  },
  "homepage": "https://github.com/Snowfly-T/graphql-intuitive-request",
  "description": "",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && cpy ./src/types/**/*.d.ts ./build/types && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && prettier --write ./build/**/*.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint . --ext .js,.ts"
  },
  "type": "module",
  "types": "./index.d.ts",
  "typesVersions": {
    ">=5": {
      "*": [
        "./build/types/ts5/*"
      ]
    },
    ">=4": {
      "*": [
        "./build/types/ts4/*"
      ]
    }
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.3.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0-beta"
  },
  "dependencies": {
    "graphql-request": "^5.2.0"
  }
}
